node {
  environment {
    aquascanner = "aquaMicroscanner imageName: 'lizrice/struts:vulnerable', notCompliesCmd: 'exit 1', onDisallowed: 'ignore', outputFormat: 'json'"
    docker-server="192.168.0.248"
    }
    stage("clone"){
        sh "echo clone"
        sh "whoami"
    }
    stage("Unit Test"){
        sh "echo Unit Test"
    }
    stage("Build"){
        sh "echo Build"
    }
    stage("Aqua Microscanner"){
        sh 'ssh root@dcoker-server "$aquascanner"'
        //aquaMicroscanner imageName: 'lizrice/struts:vulnerable', notCompliesCmd: 'exit 1', onDisallowed: 'ignore', outputFormat: 'json'
    }
    stage("Integration Test"){
        sh "echo Push Image"
    }
    stage("Deploy"){
        sh "echo Deploy Image"
    }
}
