pipeline {
    agent any
    environment {
      tomcatserver = '192.168.0.21'
    }
    
    stages {
        stage('Aqua Scanner') {
          steps {
            aquaMicroscanner imageName: '', notCompliesCmd: '', onDisallowed: 'ignore', outputFormat: 'json'
                sh ''
            }
          }
        }
      }  
