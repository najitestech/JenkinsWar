pipeline {
    agent any
    environment {
      docker = '192.168.0.248'
    }
    
    stages {
        stage('Stage1') {
          steps {
            //aquaMicroscanner imageName: 'alpine:latest', notCompliesCmd: 'exit 1', onDisallowed: 'ignore', outputFormat: 'json'
                sh 'ssh root@$docker "docker ps -a"'
            }
          }
    stage('Stage2') {
          steps {
              sh 'ssh root@$docker "docker images"'
            }
          }      
        }
      }
